{
  "root": "public",
  "rewrites": [
    {
      "source": "/Tetris",
      "destination": "/Tetris/tetris.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
